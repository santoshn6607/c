#include <stdio.h>
#include <stdlib.h>

int main() {
    int m, n, p, q, i, j, k;
    int mat1[10][10], mat2[10][10], prod[10][10] = {0};

    printf("Enter order of Matrix 1 (rows cols): ");
    scanf("%d%d", &m, &n);

    printf("Enter order of Matrix 2 (rows cols): ");
    scanf("%d%d", &p, &q);

    if(n != p) {
        printf("Matrix multiplication not possible\n");
        return 0;
    }

    printf("Enter elements of Matrix 1:\n");
    for(i = 0; i < m; i++)
        for(j = 0; j < n; j++)
            scanf("%d", &mat1[i][j]);

    printf("Enter elements of Matrix 2:\n");
    for(i = 0; i < p; i++)
        for(j = 0; j < q; j++)
            scanf("%d", &mat2[i][j]);

    for(i = 0; i < m; i++)
        for(j = 0; j < q; j++)
            for(k = 0; k < n; k++)
                prod[i][j] += mat1[i][k] * mat2[k][j];

    printf("Product of the matrices:\n");
    for(i = 0; i < m; i++) {
        for(j = 0; j < q; j++)
            printf("%d ", prod[i][j]);
        printf("\n");
    }

    return 0;
}

****************************************************OUTPUT**************************

Enter order of Matrix 1 (rows cols): 
2 2
Enter order of Matrix 2 (rows cols): 
2 2
Enter elements of Matrix 1:
1 2
2 3
Enter elements of Matrix 2:
1 3
2 1
Product of the matrices:
5 5
8 9

Process returned 0 (0x0)   execution time : 37.765 s
